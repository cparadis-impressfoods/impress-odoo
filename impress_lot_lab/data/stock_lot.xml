<?xml version='1.0' encoding="utf-8"?>
<odoo>
    <data>
        <record id='x_lab_state' model='ir.model.fields'>
            <field name="name">x_lab_state</field>
            <field name="field_description">Laboratory_state</field>
            <field name="model_id" model="ir.model" eval="obj().env.ref('stock.model_stock_lot').id"/>
            <field name="ttype">selection</field>
            <field name="selection">[('not_required', 'Not Required'), ('sent', 'Sent'),  ('received_ok', 'Received and OK'), ('received_problem', 'Received and follow-up needed')]</field>
        </record>
    </data>
</odoo>